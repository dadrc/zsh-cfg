alias cupsa='echo "Forwarding linuxservera:631 to localhost:1631"; ssh -N -L 1631:localhost:631 linuxservera'
alias cupss='echo "Forwarding linuxservers:631 to localhost:1631"; ssh -N -L 1631:localhost:631 linuxservers'
